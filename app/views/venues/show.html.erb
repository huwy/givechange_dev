<!-- Main -->
<div id="main">
	<div id="main-top">&nbsp;</div>
	<div id="main-cnt">
		<div id="venue-profile">
			<div class="cl">&nbsp;</div>
			<div class="quote">
				<p>“<%= @venue.quotation %>”</p>
			</div>
			<h2><%= @venue.name %></h2>
			<div class="cl">&nbsp;</div>
        <%= render(:partial => 'info', :object => @venue) %>
			<div class="main-side">
				<div class="top">
					<h3>Venue Profile</h3>
					<div class="slider">
						<div class="buttons">
							<a href="#" class="prev">prev</a>
							<a href="#" class="next">next</a>
						</div>
						<div class="holder">&nbsp;</div>
					</div>
					<div class="holder like-slider">&nbsp;</div>
					<h4>Who Likes It (<%= @venue.favorite_count %>):</h4>
					<div class="slider user-slider">
						<div class="buttons">
							<a href="#" class="prev">prev</a>
							<a href="#" class="next">next</a>
						</div>
						<ul>
						    <li>
						    	<div class="holder">&nbsp;</div>
						    	<p>[Info]</p>
						    </li>
						    <li>
						    	<div class="holder">&nbsp;</div>
						    	<p>[Info]</p>
						    </li>
						    <li>
						    	<div class="holder">&nbsp;</div>
						    	<p>[Info]</p>
						    </li>
						    <li>
						    	<div class="holder">&nbsp;</div>
						    	<p>[Info]</p>
						    </li>
						</ul>
					</div>
				</div>
			</div>
			<div class="cl">&nbsp;</div>
		</div>
	</div>
	<div id="main-bot">&nbsp;</div>
</div>
